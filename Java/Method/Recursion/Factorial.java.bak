import java.util.Scanner;
class Factorial 
{
	public static void main(String[] args) 
	{
		System.out.println("Enter a number");
		Scanner a= new Scanner(System.in);
		int b=a.nextInt();
		int c=fact(b);
		System.out.println("factorial="+c);
	}
	static int fact(int n)
	{
		if(n==0) return 1;
		return fact(n-1)*n;
	}
}
